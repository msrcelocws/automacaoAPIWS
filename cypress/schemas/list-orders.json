{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Partner Orders List",
    "type": "array",
    "items": {
        "type": "object",
        "required": [
            "id",
            "orderId",
            "status",
            "dateCreated"
        ],
        "properties": {
            "id": {
                "type": "integer"
            },
            "orderId": {
                "type": "integer"
            },
            "total": {
                "type": [
                    "string",
                    "null"
                ]
            },
            "status": {
                "type": "string",
                "enum": [
                    "AWAITING_PAYMENT",
                    "PAYMENT_APPROVED",
                    "CANCELED",
                    "PRE_ORDER",
                    "SHIPPED",
                    "DELIVERED"
                ]
            },
            "dateCreated": {
                "type": "string",
                "format": "date-time"
            },
            "lastUpdated": {
                "type": [
                    "string",
                    "null"
                ],
                "format": "date-time"
            },
            "gateway": {
                "type": [
                    "string",
                    "null"
                ]
            },
            "partner": {
                "type": "object",
                "properties": {
                    "partnerName": {
                        "type": "string"
                    },
                    "partnerDocument": {
                        "type": "string"
                    }
                }
            },
            "customer": {
                "type": "object",
                "properties": {
                    "customerName": {
                        "type": "string"
                    },
                    "customerDocument": {
                        "type": "string"
                    }
                }
            }
        }
    }
}