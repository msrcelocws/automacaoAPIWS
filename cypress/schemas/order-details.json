{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Order Details",
    "oneOf": [
        {
            "type": "object",
            "$ref": "#/definitions/orderObject"
        },
        {
            "type": "array",
            "items": {
                "$ref": "#/definitions/orderObject"
            },
            "minItems": 1
        }
    ],
    "definitions": {
        "orderObject": {
            "type": "object",
            "required": [
                "orderId",
                "id",
                "status",
                "partnerOrderAttributes",
                "deliveries",
                "paymentDateVpvf"
            ],
            "properties": {
                "orderId": {
                    "type": "integer"
                },
                "id": {
                    "type": "integer"
                },
                "status": {
                    "type": "string"
                },
                "paymentDateVpvf": {
                    "type": "string"
                },
                "partnerOrderAttributes": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "required": [
                            "name",
                            "value"
                        ],
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "value": {
                                "type": "string"
                            }
                        }
                    }
                },
                "deliveries": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "required": [
                            "products"
                        ],
                        "properties": {
                            "products": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "required": [
                                        "id",
                                        "name",
                                        "priceContractSkuCustomAttributes"
                                    ],
                                    "properties": {
                                        "id": {
                                            "type": "integer"
                                        },
                                        "name": {
                                            "type": "string"
                                        },
                                        "priceContractSkuCustomAttributes": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "required": [
                                                    "name",
                                                    "value"
                                                ],
                                                "properties": {
                                                    "name": {
                                                        "type": "string"
                                                    },
                                                    "value": {
                                                        "type": "string"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}